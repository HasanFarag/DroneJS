  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/gandhimonik/DroneJS/issues"
  },
  "homepage": "https://github.com/gandhimonik/DroneJS#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "noble": "^1.8.0",
    "rxjs": "^5.3.0",
    "sinon": "^2.1.0"
  }
