  "description": "A Node.js based library for controlling a Parrot Rolling Spider drone",
  "main": "index.js",
  "debugging": true,
  "scripts": {
    "test": "mocha --compilers js:babel-register --require babel-polyfill --recursive ./test/views/MiniDroneViewSpec.js --timeout 500000",
    "start": "mocha --compilers js:babel-register --require babel-polyfill --reporter spec --inspect --debug-brk ./test/views/MiniDroneViewSpec.js"
